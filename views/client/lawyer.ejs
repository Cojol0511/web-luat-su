      <!-- End Tab Section -->

      <!-- ======= Team Section ======= -->
      <section id="team" class="team">
        <div class="container">
          <div class="section-title">
            <h2>Danh sách luật sư khác</h2>
            <p>Một số các luật sư cấp cao ở công ty chúng tôi</p>
          </div>

          <div class="row">
            <div class="owl-carousel team-slider">
              <% for (var i=0; i < lawyer_list.length; i++){ other_lawyer =
              lawyer_list[i]; if (other_lawyer.LawyerID != lawyer.LawyerID){ %>
              <div class="col-12">
                <div class="member">
                  <img src="<%= other_lawyer.Avatar %>" alt="" />
                  <div class="card-body">
                    <h4>
                      <%= other_lawyer.FirstName %> <%= other_lawyer.LastName %>
                    </h4>
